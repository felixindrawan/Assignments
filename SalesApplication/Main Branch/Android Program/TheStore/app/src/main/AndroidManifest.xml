<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.thestore">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/mojoe_ic"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/mojoe_ic_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme.Launcher">
    <activity android:name=".InitActivity"></activity>
    <activity android:name=".CustomerActivity" />
    <activity android:name=".EmployeeActivity" />
    <activity android:name=".AdminActivity" />
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>